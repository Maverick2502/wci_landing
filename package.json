{
  "name": "landing",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro-i18next generate && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "i18n:generate": "astro-i18next generate",
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    }
  },
  "lint-staged": {
    "*.{astro,ts, tsx}": [
      "prettier --write ./src"
    ],
    "*.{md,html,scss,json}": "prettier --write"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/react": "^3.1.1",
    "@fontsource-variable/geologica": "^5.0.4",
    "@fontsource-variable/inter": "^5.0.17",
    "@motionone/utils": "^10.17.0",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "astro": "^4.5.16",
    "astro-i18next": "^1.0.0-beta.21",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.15",
    "i18next": "^22.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^12.2.0",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "astro-eslint-parser": "^0.16.3",
    "husky": "^8.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "sass": "^1.71.1"
  }
}
